public class SumDivisors {
	public static void main (String[] args){
        int n = Integer.parseInt(args[0]);
	int sum = 1;
		
	for (int i = 2; i <= Math.sqrt(n); i++){
		if(n % i == 0) {
			sum += i + n/i;
			}
	}
		
	if (Math.sqrt(n) % 1 == 0){
		sum -= Math.sqrt(n);
		}
		
		System.out.println(sum);
	}
}
